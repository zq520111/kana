<header class="site-header">
<hgroup>
      <h1 class="site-title">
        <a href="<%= config.url %>" class="logo"><%= config.title %></a>
      </h1>
      <p class="site-description"><%= config.description %></p>
    </hgroup>
    <div class="site-nav">
      <ul class="topNav-items">
        <% for (var i in theme.menu){ %>
          <li class="menu-item"><a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a></li>
        <% } %>
      </ul>
    </div>
    <div class="site-icons">
      <a href="<%- theme.rss %>" class="rss-link"><svg class="icon icon-rss"><use xlink:href="#icon-rss"></use></svg></a>
    </div>
</header>